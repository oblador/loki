{
  "gitbook": "3.2.2",
  "title": "Loki",
  "root": "docs",
  "plugins": [
    "edit-link",
    "theme-gestalt",
    "-theme-default",
    "prism",
    "-highlight",
    "anchorjs",
    "github",
    "styles-sass-fix"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/oblador/loki/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/oblador/loki/"
    },
    "theme-gestalt": {
      "logo": "logo.svg",
      "favicon": "favicon.png",
      "excludeDefaultStyles": false
    }
  },
  "styles": {
    "website": "./website.scss"
  }
}
