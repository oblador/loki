{
  "name": "loki",
  "version": "0.9.0",
  "description": "Visual Regression Testing for react-storybook",
  "main": "index.js",
  "bin": {
    "loki": "bin/loki"
  },
  "files": [
    "README.md",
    "LICENCE",
    "src",
    "bin",
    "configure-react.js",
    "configure-react-native.js",
    "configure-vue.js"
  ],
  "scripts": {
    "serve-docs": "gitbook serve",
    "build-docs": "(gitbook install || (npm rebuild node-sass && gitbook install)) && gitbook build",
    "publish-docs": "./bin/publish-docs",
    "format": "npm run lint -- --fix",
    "lint": "eslint *.js src",
    "test": "jest"
  },
  "engines": {
    "node": ">=7.6"
  },
  "keywords": [
    "storybook",
    "react-storybook",
    "testing",
    "visual testing",
    "visual testing",
    "react-native",
    "react",
    "mobile",
    "ios",
    "android",
    "web"
  ],
  "author": {
    "name": "Joel Arvidsson",
    "email": "joel@oblador.se"
  },
  "homepage": "https://loki.js.org",
  "bugs": {
    "url": "https://github.com/oblador/loki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oblador/loki.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.0.1",
    "chrome-remote-interface": "^0.23.3",
    "debug": "^2.6.8",
    "execa": "^0.7.0",
    "fs-extra": "^3.0.1",
    "get-port": "^3.2.0",
    "gitbook-plugin-anchorjs": "^1.1.1",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-github": "^2.0.0",
    "gitbook-plugin-prism": "^2.3.0",
    "gitbook-plugin-theme-gestalt": "^1.0.2",
    "gm": "^1.23.0",
    "jsdom": "^11.5.1",
    "lighthouse": "^2.2.1",
    "listr": "^0.12.0",
    "looks-same": "^3.2.1",
    "minimist": "^1.2.0",
    "osnap": "^1.1.0",
    "ramda": "^0.24.1",
    "shelljs": "^0.7.8",
    "transliteration": "^1.6.2",
    "wait-on": "^2.0.2",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "gitbook-cli": "^2.3.2",
    "jest": "^20.0.4",
    "prettier": "^1.5.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  }
}
